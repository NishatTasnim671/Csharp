        <TextBox Name="page" HorizontalAlignment="Left" Height="23" Margin="45.4,99.2,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.552,0.6" Grid.ColumnSpan="2" Grid.Column="1" Grid.Row="1"/>
        <TextBox Name="pdate" HorizontalAlignment="Left" Height="23" Margin="56.4,175.2,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="3" Grid.Column="1" Grid.Row="1" RenderTransformOrigin="0.393,0.226"/>
        <TextBox Name="phospital" HorizontalAlignment="Left" Height="23" Margin="458,276.2,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="1.505,-1.765" Grid.Row="1"/>
