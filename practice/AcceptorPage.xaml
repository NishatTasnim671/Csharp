                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="4.899"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.549"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button Content="Back" Grid.Column="3" HorizontalAlignment="Left" Margin="42,264.2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="58" Click="Button_Click" Height="35"/>
        <Button Content="LogOut" Grid.Column="3" HorizontalAlignment="Left" Margin="42,335.2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="58" Click="Button_Click_1" Height="26" RenderTransformOrigin="0.597,1.238"/>

