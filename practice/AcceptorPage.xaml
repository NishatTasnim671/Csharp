        <Label Content="Covid positive Date" HorizontalAlignment="Left" Margin="45.4,141.2,0,0" VerticalAlignment="Top" Height="29" Width="112" RenderTransformOrigin="-0.866,1.785" Grid.ColumnSpan="2" Grid.Column="1" Grid.Row="1"/>
        <Label Content="Name of Hospital admit at" HorizontalAlignment="Left" Margin="458,226.2,0,0" VerticalAlignment="Top" Height="26" Width="150" RenderTransformOrigin="1.167,4.631" Grid.Row="1"/>
