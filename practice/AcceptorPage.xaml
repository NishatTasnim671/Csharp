            <RowDefinition Height="409*"/>
        </Grid.RowDefinitions>
        <Label Content="Please Provide Your Medical information" FontSize="16" HorizontalAlignment="Left" Margin="608,9.2,0,0" VerticalAlignment="Top" Height="29" Width="294" Grid.ColumnSpan="3" Grid.Row="1" RenderTransformOrigin="0.688,0.414"/>
        <Label Content="Patient Name:" HorizontalAlignment="Left" Margin="458,68.2,0,0" VerticalAlignment="Top" Height="26" Width="85" RenderTransformOrigin="0.666,2.2" Grid.Row="1"/>
